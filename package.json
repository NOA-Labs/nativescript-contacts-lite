{
  "name": "nativescript-contacts-lite",
  "version": "0.3.0",
  "description": "NativeScript plugin providing pretty fast read-only access to the iOS and Android contact directory",
  "main": "index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhayastudios/nativescript-contacts-lite.git"
  },
  "keywords": [
    "NativeScript",
    "Contacts",
    "Address Book",
    "Contact Directory"
  ],
  "author": {
    "name": "Jonathan Salomon",
    "email": "mail2joni@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abhayastudios/nativescript-contacts-lite/issues"
  },
  "homepage": "https://github.com/abhayastudios/nativescript-contacts-lite",
  "nativescript": {
    "platforms": {
      "ios": "7.0.3",
      "android": "7.0.1"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "webpack": "true",
      "wrapper": "false",
      "category": "Processing"
    },
    "demong": "/demo-angular"
  },
  "dependencies": {
    "nativescript-permissions": "^1.3.11"
  },
  "devDependencies": {
    "@nativescript/core": "^7.0.12",
    "@nativescript/types": "^7.0.4",
    "@nativescript/webpack": "^3.0.8",
    "nativescript-worker-loader": "git+https://7b8666419efea0fa144da0c1f42f6f69d4793958:x-oauth-basic@github.com/NOA-Labs/worker-loader.git",
    "typescript": "^4.0.5"
